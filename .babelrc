{
  "env": {
    "production": {
      "ignore": [
        "./src/**/*.spec.jsx",
        "./src/**/*.spec.tsx",
        "./src/**/*.spec.js",
        "./src/**/*.spec.ts",
        "./src/**/*.stories.jsx",
        "./src/**/*.stories.tsx",
        "./src/**/*.stories.js",
        "./src/**/*.stories.ts"
      ],
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ]
    },
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
